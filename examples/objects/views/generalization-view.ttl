@prefix ex: <http://example.com/>.
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.

ex:gv1 a ontouml:GeneralizationView ;
      ontouml:id "gv1" ; 
      ontouml:isViewOf ex:g1 ;
      ontouml:shape ex:path1 ; 
      ontouml:project ex:p1 .

ex:p1 a ontouml:Project .
ex:g1 a ontouml:Generalization .
ex:path1 a ontouml:Path .