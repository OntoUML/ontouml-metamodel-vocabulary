@prefix : <https://example.org#> .
@prefix ontouml: <https://w3id.org/ontouml#> .

:gv a ontouml:GeneralizationView ;
    ontouml:isViewOf :g ;
    ontouml:project :p ;
    ontouml:shape :pa ;
    ontouml:sourceView :cv1 ;
    ontouml:targetView :cv2 .

:g a ontouml:Generalization .
:pa a ontouml:Path .
:cv1 a ontouml:ClassView .
:cv2 a ontouml:ClassView .
:p a ontouml:Project .