@prefix ex: <http://example.com/>.
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.

ex:gsv1 a ontouml:GeneralizationSetView ;
      ontouml:id "gsv1" ; 
      ontouml:isViewOf ex:c1 ;
      ontouml:shape ex:txt1 ; 
      ontouml:project ex:p1 .

ex:p1 a ontouml:Project .
ex:c1 a ontouml:GeneralizationSet .
ex:txt1 a ontouml:Text .