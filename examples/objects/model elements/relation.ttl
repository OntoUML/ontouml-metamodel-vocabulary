@prefix : <https://example.org#> .
@prefix ontouml: <https://w3id.org/ontouml#> .

:r a ontouml:Relation ;
    ontouml:name "inheres in"@en ;
    ontouml:stereotype ontouml:characterization ;
    ontouml:isAbstract false ;
    ontouml:isDerived false ;
    ontouml:relationEnd :pr1, :pr2 ;
    ontouml:sourceEnd :pr1 ;
    ontouml:targetEnd :pr2 ;
    ontouml:project :p .

:pr1 a ontouml:Property .
:pr2 a ontouml:Property .
:p a ontouml:Project .