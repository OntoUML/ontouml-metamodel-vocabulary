@prefix ex: <http://example.com/>.
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.

ex:pk1 a ontouml:Package ;
      ontouml:id "pk1" ; 
      ontouml:name "Main module"@en ; 
      ontouml:description "The main module of my ontology."@en ;
      ontouml:containsModelElement ex:pk2, ex:c1, ex:r1, ex:g1 ;
      ontouml:project ex:p1 .

ex:p1 a ontouml:Project .
ex:pk2 a ontouml:Package .
ex:c1 a ontouml:Class .
ex:r1 a ontouml:Relation .
ex:g1 a ontouml:Generalization .
