@prefix : <https://example.org#> .
@prefix ontouml: <https://w3id.org/ontouml#> .

:pr a ontouml:Property ;
    ontouml:name "father"@en ;
    ontouml:aggregationKind ontouml:none ;
    ontouml:cardinality :ca ;
    ontouml:isDerived false ;
    ontouml:isOrdered false ;
    ontouml:isReadOnly false ;
    ontouml:project :p ;
    ontouml:propertyType :c .

:ca a ontouml:Cardinality .
:p a ontouml:Project .
:c a ontouml:Class .