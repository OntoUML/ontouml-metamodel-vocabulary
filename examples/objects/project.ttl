@prefix ex: <http://example.com/>.
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.

ex:p1 a ontouml:Project;
      ontouml:name "Common Ontology of Value and Risk"@en ;
      ontouml:description "COVER is a well-founded ontology that makes the deep connections between the concepts of value and risk explicit. COVER is grounded on several theories from marketing, service science, strategy and risk management."@en ;
      ontouml:model ex:pk1 ;
      ontouml:diagram ex:d1, ex:d2 .

ex:pk1 a ontouml:Package .
ex:d1 a ontouml:Diagram .
ex:d2 a ontouml:Diagram .
