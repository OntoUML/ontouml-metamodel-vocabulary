@prefix ex: <http://example.com/>.
@prefix ontouml: <https://purl.org/ontouml-models/vocabulary/>.

ex:path1 a ontouml:Path ;
         ontouml:id "rect1" ; 
         ontouml:point (
            [ ontouml:x "10" ; ontouml:y "10"  ]
            [ ontouml:x "10" ; ontouml:y "50"  ]
            [ ontouml:x "90" ; ontouml:y "50"  ]
            [ ontouml:x "100" ; ontouml:y "60"  ]
         ) .
